{
  "name": "tesseract-coding-challenge",
  "engines": {
    "node": "^22.0.0",
    "npm": "^10.0.0"
  },
  "main": "main.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "start": "node dist/main.js",
    "watch": "nodemon --exec \"node --require ts-node/register --inspect=0.0.0.0:9229 src/main.ts\"",
    "test:ci": "npm run util:typeCheck && LOG_LEVEL=silent node node_modules/.bin/jest --collectCoverage --ci",
    "test:local": "LOG_LEVEL=info node --inspect=0.0.0.0:9228 node_modules/.bin/jest --runInBand --verbose --detectOpenHandles",
    "test:watch": "LOG_LEVEL=silent node --inspect=0.0.0.0:9228 node_modules/.bin/jest --runInBand --watch",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "prettier": "prettier --check \"src/**/*.{js,ts}\"",
    "util:typeCheck": "tsc --noEmit -p ./tsconfig.json"
  },
  "dependencies": {
    "@swc/core": "^1.13.5",
    "dotenv": "^16.0.0",
    "express": "^4.0.0",
    "joi": "^17.13.3",
    "pg": "^8.16.3",
    "pino": "^9.0.0",
    "sequelize": "^6.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/express": "^4.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "20.0.0",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jest": "^29.0.0",
    "nock": "^13.0.0",
    "nodemon": "^3.0.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.0.0",
    "sqlite3": "^5.0.0",
    "supertest": "^6.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  }
}
